# NWWS-OI Configuration
NWWS_USERNAME=your_username_here
NWWS_PASSWORD=your_password_here
#NWWS_SERVER=nwws-oi.weather.gov
#NWWS_PORT=5222

# Logging Configuration
LOG_LEVEL=INFO
# LOG_FILE=/path/to/log/file.log

# Statistics Configuration
STATS_INTERVAL=60

# Prometheus Metrics Configuration
METRICS_ENABLED=true          # Enable Prometheus metrics endpoint
METRICS_PORT=8080            # Port for metrics endpoint
METRICS_UPDATE_INTERVAL=30   # Update metrics every 30 seconds

# Output Configuration
# Comma-separated list of output handlers: console, mqtt
OUTPUT_HANDLERS=console

# MQTT Configuration (only needed if mqtt is in OUTPUT_HANDLERS)
# MQTT_BROKER=your.mqtt.broker.com
# MQTT_PORT=1883
# MQTT_USERNAME=mqtt_user
# MQTT_PASSWORD=mqtt_password
# MQTT_TOPIC_PREFIX=nwws
# MQTT_QOS=1
# MQTT_RETAIN=true
# MQTT_CLIENT_ID=nwws-oi-client

DASHBOARD_ENABLED=true  # Enable or disable the dashboard
# DASHBOARD_PORT=5000  # Port for the dashboard (default is 5000)
# DASHBOARD_HOST="localhost"  # Host for the dashboard (default is localhost)